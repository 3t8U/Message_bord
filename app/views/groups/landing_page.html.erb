<h3>Latest Groups</h3>
<% @groups.three_most_recent.each do |three_most_recentGroup| %>
<p><%= link_to three_most_recentGroup.name, group_path(three_most_recentGroup) %></p>
<% end %>

<h3>Group with the most Reviews</h3>
<% @groups.most_messages.each do |most_messagesGroup| %>
<p><%= link_to most_messagesGroup.name, group_path(most_messagesGroup) %></p>
<% end %>
